{
    "name": "andersoncorso/cakephp-plugin-access_manager",
    "description": "AccessManager plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "ivanamat/cakephp3-aclmanager": "1.*"
    },
	"require-dev": {
	},
	"suggest": {
		"maiconpinto/cakephp-adminlte-theme": "Recomenda-se o uso do plugin AdminLTE"
	},
    "autoload": {
        "psr-4": {
            "AccessManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AccessManager\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "minimum-stability": "dev",    
    "prefer-stable": true
}
